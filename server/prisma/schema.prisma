generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      String      @id @default(cuid())
  email   String   @unique
  password String
}

model UserToken {
  id String @id
  accessToken String
  refreshToken String
}